      }      
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": { 
        "breadcrumb_path": "~/breadcrumb/toc.yml"
     },
    "fileMetadata": {},
    "template": [
      null
    ],
    "dest": "system-center",
    "groups": {
      "dpm": {
        "dest": "dpm",
        "moniker_range": ">= sc-dpm-2016"
      },
      "orchestrator": {
        "dest": "orchestrator",
        "moniker_range": ">= sc-orch-2016"
      },
      "scom": {
