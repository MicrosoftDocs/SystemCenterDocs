{
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "dpm/**/*.md",
          "dpm/**/*.yml"
        ],
        "group": "dpm",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "orchestrator/**/*.md",
          "orchestrator/**/*.yml"
        ],
        "group": "orchestrator",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "scom/**/*.md",
          "scom/**/*.yml"
        ],
        "group": "scom",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "scsm/**/*.md",
          "scsm/**/*.yml"
        ],
        "group": "scsm",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "sma/**/*.md",
          "sma/**/*.yml"
        ],
        "group": "sma",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "spf/**/*.md",
          "spf/**/*.yml"
        ],
        "group": "spf",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "vmm/**/*.md",
          "vmm/**/*.yml"
        ],
        "group": "vmm",
        "exclude": [
          "**/obj/**",         
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }

    ],
    "resource": [
      {
        "files": [
          "dpm/**/*.png",
          "dpm/**/*.jpg",
          "dpm/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "dpm"
      },
      {
        "files": [
          "orchestrator/**/*.png",
          "orchestrator/**/*.jpg",
          "orchestrator/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "orchestrator"
      },
      {
        "files": [
          "scom/**/*.png",
          "scom/**/*.jpg",
          "scom/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "scom"
      },
      {
        "files": [
          "scsm/**/*.png",
          "scsm/**/*.jpg",
          "scsm/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "scsm"
      },
      {
        "files": [
          "spf/**/*.png",
          "spf/**/*.jpg",
          "spf/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "spf"
      },
      {
        "files": [
          "vmm/**/*.png",
          "vmm/**/*.jpg",
          "vmm/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "vmm"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": { 
        "breadcrumb_path": "~/breadcrumb/toc.yml"
     },
    "fileMetadata": {},
    "template": [
      null
    ],
    "dest": "system-center",
    "groups": {
      "dpm": {
        "dest": "dpm",
        "moniker_range": ">= sc-dpm-2016"
      },
      "orchestrator": {
        "dest": "orchestrator",
        "moniker_range": ">= sc-orch-2016"
      },
      "scom": {
        "dest": "scom",
        "moniker_range": ">= sc-om-2016"
      },
      "scsm": {
        "dest": "scsm",
        "moniker_range": "> sc-sm-2016"
      },
      "sma": {
        "dest": "sma",
        "moniker_range": "> sc-sma-2016"
      },
      "spf": {
        "dest": "spf",
        "moniker_range": "> sc-spf-2016"
      },
      "vmm": {
        "dest": "vmm",
        "moniker_range": "> sc-vmm-2016"
      }
    }
  }
}
